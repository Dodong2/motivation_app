{
  "expo": {
    "name": "StudySprint",
    "slug": "student",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bookSplash.png",
    "scheme": "student",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationAlertUsageDescription": "This app uses notifications to keep you focused."
      }
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS",
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bookIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dodong_arocha.student"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bookSplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "06e7daaf-e382-4bd3-bbcc-004fdd87d214"
      }
    },
    "owner": "dodong_arocha"
  }
}
